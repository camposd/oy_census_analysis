oy_household_collapse <- function(oy_household){
# Collapsing OY-Only
# OY-CY ONLY
# OY-EE Only
out <-
forcats::fct_collapse(
`OY Household` = c("OY Only Household",
"OY-CY Only Household",
"OY-EE Only Household"))
return(out)
}
test <- analysis_data$household_income_estimates$oy_household_full %>% unique()
oy_household_collapse(test)
oy_household_collapse <- function(oy_household){
# Collapsing OY-Only
# OY-CY ONLY
# OY-EE Only
out <-
forcats::fct_collapse(
.f = oy_household,
`OY Household` = c("OY Only Household",
"OY-CY Only Household",
"OY-EE Only Household"))
return(out)
}
